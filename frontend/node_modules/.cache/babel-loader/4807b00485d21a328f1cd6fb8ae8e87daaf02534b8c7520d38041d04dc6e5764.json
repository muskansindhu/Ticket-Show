{"ast":null,"code":"import SearchShowPage from \"@/components/SearchShow.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"SearchShowView\",\n  components: {\n    SearchShowPage\n  },\n  props: {\n    term: String\n  },\n  data() {\n    return {\n      shows: []\n    };\n  },\n  created() {\n    this.fetchSearchResults(this.term);\n  },\n  methods: {\n    async fetchSearchResults(term) {\n      const response = await axios.get(`http://127.0.0.1:1234/vue/searchShows/${term}`);\n      this.shows = response.data.shows;\n    },\n    async beforeMount() {\n      await this.fetchShowData(); // Call the fetchShowData method\n    }\n  },\n\n  computed: {\n    boolShows() {\n      if (this.shows) {\n        return this.shows.length > 0;\n      }\n      return false;\n    },\n    compShows() {\n      return this.shows;\n    }\n  }\n};","map":{"version":3,"names":["SearchShowPage","axios","name","components","props","term","String","data","shows","created","fetchSearchResults","methods","response","get","beforeMount","fetchShowData","computed","boolShows","length","compShows"],"sources":["src/views/SearchShow.vue"],"sourcesContent":["<template>\n  <div class=\"container\" style=\"padding-top: 44px\">\n    <div\n      class=\"bg-dark border rounded border-0 border-dark overflow-hidden\"\n      style=\"padding-bottom: 0px; margin-bottom: 64px\"\n    >\n      <div class=\"p-md-3\" v-if=\"this.boolShows\">\n        <p class=\"fw-bold text-white mb-2\">\n          Shows found for the search term are as follows:\n        </p>\n      </div>\n      <div v-if=\"this.boolShows\">\n        <div v-for=\"show in this.compShows\" :key=\"show.roll\">\n          <SearchShowPage :show=\"show\"></SearchShowPage>\n        </div>\n      </div>\n      <div class=\"p-md-3\" v-if=\"!this.boolShows\">\n        <p class=\"fw-bold text-white mb-3\">\n          No shows found for the search term\n        </p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport SearchShowPage from \"@/components/SearchShow.vue\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"SearchShowView\",\n  components: { SearchShowPage },\n  props: {\n    term: String,\n  },\n  data() {\n    return {\n      shows: [],\n    };\n  },\n  created() {\n    this.fetchSearchResults(this.term);\n  },\n  methods: {\n    async fetchSearchResults(term) {\n      const response = await axios.get(\n        `http://127.0.0.1:1234/vue/searchShows/${term}`\n      );\n      this.shows = response.data.shows;\n    },\n    async beforeMount() {\n      await this.fetchShowData(); // Call the fetchShowData method\n    },\n  },\n  computed: {\n    boolShows() {\n      if (this.shows) {\n        return this.shows.length > 0;\n      }\n      return false;\n    },\n    compShows() {\n      return this.shows;\n    },\n  },\n};\n</script>\n"],"mappings":"AA0BA,OAAAA,cAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAH;EAAA;EACAI,KAAA;IACAC,IAAA,EAAAC;EACA;EACAC,KAAA;IACA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,kBAAA,MAAAL,IAAA;EACA;EACAM,OAAA;IACA,MAAAD,mBAAAL,IAAA;MACA,MAAAO,QAAA,SAAAX,KAAA,CAAAY,GAAA,CACA,yCAAAR,IAAA,EACA;MACA,KAAAG,KAAA,GAAAI,QAAA,CAAAL,IAAA,CAAAC,KAAA;IACA;IACA,MAAAM,YAAA;MACA,WAAAC,aAAA;IACA;EACA;;EACAC,QAAA;IACAC,UAAA;MACA,SAAAT,KAAA;QACA,YAAAA,KAAA,CAAAU,MAAA;MACA;MACA;IACA;IACAC,UAAA;MACA,YAAAX,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}